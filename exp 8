alphabet <- letters
first_10_lower <- alphabet[1:10]
last_10_upper <- toupper(alphabet[17:26])
letters_22_to_24_upper <- toupper(alphabet[22:24])
cat("First 10 lowercase letters:", first_10_lower, "\n")
cat("Last 10 uppercase letters:", last_10_upper, "\n")
cat("Letters between 22 and 24 in uppercase:", letters_22_to_24_upper, "\n")
